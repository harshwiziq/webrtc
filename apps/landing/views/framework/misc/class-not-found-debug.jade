doctype html
html( lang="en" )
	head
		title WizIQ Error Information
		meta( charset='utf-8' )
		meta( http-equiv='X-UA-Compatible', content='IE=edge' )
		meta( name='viewport', content='width=device-width, initial-scale=1.0' )
		meta( name='description', content='WizIQ Virtual Classroom' )
		link(href='/landing/stylesheets/error-pages01.css', rel='stylesheet' )
		script(src='/landing/javascripts/ext/jquery-1.11.3.min.js', type='text/javascript' )
		script(src='/landing/javascripts/ext/moment.min.js', type='text/javascript' )
		script(src='/landing/javascripts/framework/misc/class-not-found-debug.js', type='text/javascript' )
	body
		.wrapper.row2
			#container.clear
				section#fof.clear
					.hgroup
						h1.main Provisioning says
						h2.secondary #{err}

					// Details

					table.details
						thead
							tr
								th Key
								th Value
						tbody
							tr
								td Status
								td.text-danger #{backend_config.class_config.status}
							tr
								td Class ID
								td #{backend_config.class_config.class_id}
							tr
								td Title 
								td #{backend_config.class_config.meta_info.title}
							tr
								td Scheduled Time
								td
									span.scheduled_time
									span &nbsp(
									span.starts
									span )
							tr
								td Duration
								td.duration

		script(type="text/javascript")
			|var backend_config = !{JSON.stringify(backend_config)};
